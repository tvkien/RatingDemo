{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/bootstrap/4.4.1",
      "files": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.5.0",
      "destination": "wwwroot/jquery/3.5.0",
